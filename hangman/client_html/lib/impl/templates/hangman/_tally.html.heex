<div class="hangman_overview">
    <h3 class="hangman_word"><%= @tally.hangman |> Enum.join(" ") %></h3>
    <p class="hangman_tally">
        Turns left: <%= @tally.turns_left %> <br>
        Letters used: <%= @tally.used_letters |> Enum.join(", ") %> <br>
        State: <%= @tally.state %>
    </p>
</div>
